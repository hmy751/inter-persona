// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // 또는 사용하려는 다른 데이터베이스 (mysql, sqlite, sqlserver, mongodb)
  url      = env("DATABASE_URL")
}

// 예시 모델
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
} 