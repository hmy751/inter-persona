{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "!**/*.stories.{tsx,jsx,mdx}"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build:storybook-config": {
      "dependsOn": ["^build:storybook-config"],
      "cache": false
    },
    "ui-storybook": {
      "dependsOn": ["^build:storybook-config"]
    },
    "apps-storybook": {
      "dependsOn": ["^build:storybook-config"]
    },
    "build:ui-storybook": {},
    "build:apps-storybook": {},
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:msw": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "persistent": true
    }
  }
}
